<body class="pace-done" style="">
    {% if  ShowComparisonPreview %}
    <div class="comparison-tab">
        <div class="comparison-tab-box">
            <div class="show-comparison-box-icon hidden" title="Porównaj">
                <i class="fa fa-balance-scale"></i>
            </div>
            <div class="selected-to-compare">
                <div class="title">
                    Wybrane do porównania <br>
                </div>
                <div class="item content">
                </div>
                <div class="item compare-button">
                    <span class="">
                        <a href="/Porownanie">
                            Porównaj
                        </a>
                    </span>
                </div>
                <div class="item remove-button">
                    <span class="danger">
                        <a href="javascript:void(0);" class="danger reset">
                            Wyczyść
                        </a>
                    </span>
                </div>
            </div>
        </div>
    </div>
    {% endif %}
    @BlockArea("Header")
    <nav class="navbar navbar-static-top border-bottom" style="margin-bottom: 0">
        <div class="">
            <div class="navbar-header" role="navigation">
                <a class="im" href="/" aria-hidden="true"> <img style="height: 100%; width: 100%; display: block;" class="img-fluid  m-t-xs  logo-image pull-left" {% if Company.Image_base64 == blank %}  {% else %} src="{{ResAddress}}Res/Img?o={{OrganizationId}}&b={{LogoId}}" {% endif %} style="height: 100%; width: 100%; display: block;" alt="Logo"> </a>
            </div>
            <!--<div class="navbar-header">
                <span class="navbar-minimalize minimalize-styl-2 m-t-lg ">{{Company.Name}}  {{Title}}</span>
            </div>-->
        </div>
        <div class="" style="margin-bottom:10px;">
            <div class="row">
                <!--<div class="col-lg-5">-->
                <div id="article-search" class="navbar-header input-group m-t-lg m-l-md " data-url="/Article/Search" style="{% if IsMobileDevice %} width:100%; padding-left:60px; margin-left:-30px; {% else %} width:450px; {% endif %} display: inline-table;">
                    <input type="text" name="Search" placeholder="Wyszukaj w sklepie..." class="input form-control search" value="{{Search}}" title="Wpisz szukaną frazę">
                    <ul class="result-box dropdown-menu dropdown-alerts" style="{% if IsMobileDevice %} margin-left:60px; {% endif %}display:none; position:absolute;">
                        <li class="preloader" style="display:none;">
                            <div class="sk-spinner sk-spinner-wave">
                                <div class="sk-rect1"></div>
                                <div class="sk-rect2"></div>
                                <div class="sk-rect3"></div>
                                <div class="sk-rect4"></div>
                                <div class="sk-rect5"></div>
                            </div>
                        </li>
                    </ul>

                    <span class="input-group-btn">
                        <button type="button" class="btn btn btn-primary search-button" title="Szukaj"> <i class="fa fa-search"></i></button>
                    </span>
                </div>
                <!--</div>-->
                <div class="panel-menu">
                    @BlockArea("Menu1")

                    <ul class="nav navbar-top-links navbar-right">
                        <li style="display:block" class="pull-right">
                            <form action="/Account/LogOff" style="text-align:center; margin-top:7px" id="logoutForm" method="post">
                                <form action="/Account/LogOff" style="text-align:center; margin-top:7px" id="logoutForm" method="post">
                                    {% if  UseLimits == true %}
                                    <p class="no-margins"><a href="/Rozliczenia" class="no-padding" style="min-height :0px;"><strong class="text-muted">Limit</strong> <strong class="text-danger">{{AvailableLimit}} netto</strong></a></p>
                                    {% endif %}
                                    {{LogOutAntiForgery}}
                                    <p class="no-margins"><a href="javascript:document.getElementById('logoutForm').submit()" class="no-padding"><i class="fa fa-sign-out fa-align-center "></i> Wyloguj</a></p>
                                    <p class="no-margins"><small>({{UserContext.Login}})</small></p>
                                </form>
                            </form>
                        </li>
                        <li id="cart-navbar" class="dropdown pull-right" style="display:inline-block;margin-top:10px;" data-defaultcart="{{UserContext.Default_cart}}" data-url="/Cart/GetCartList">
                            <a class="dropdown-toggle count-info" data-toggle="dropdown" href="javascript:void(0);" aria-expanded="false">
                                <i class="fa fa-shopping-cart" style="font-size:16px;"></i>  <span class="label label-primary cart-items">{{CartItems}}</span>
                            </a>
                            <ul class="dropdown-menu dropdown-alerts">
                                <li class="preloader" style="display:none;">
                                    <div class="sk-spinner sk-spinner-wave">
                                        <div class="sk-rect1"></div>
                                        <div class="sk-rect2"></div>
                                        <div class="sk-rect3"></div>
                                        <div class="sk-rect4"></div>
                                        <div class="sk-rect5"></div>
                                    </div>
                                </li>
                                <li>
                                    <div class="text-center link-block">
                                        <a class="add-cart" href="javascript:void(0);">
                                            <strong>Dodaj koszyk</strong>
                                            <i class="fa fa-plus"></i>
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    {% if  ShowTradeManager == true %}
                    <div class="pull-right" style="width:250px;white-space: nowrap;">
                        {% if  TradeManager.Display_image == true %}
                        <div style="display: inline-block;vertical-align: top;white-space: normal;">
                            <img style="width: 70px;" alt="image" class="img-circle m-t-xs img-fluid" src="{{TradeManager.Image_path}}">
                        </div>
                        {% endif %}
                        <div style="padding:unset;width:200px;display: inline-block;vertical-align: top;white-space: normal;">
                            <div class="m-t-xs font-bold"><small>Opiekun</small></div>
                            <h3 style="margin-bottom: 0px;"><strong>{{TradeManager.First_name}} {{TradeManager.Last_name}}</strong></h3>
                            <div>
                                {% if  TradeManager.Display_phone_number == true %}
                                <small><i class="fa fa-phone"></i> {{TradeManager.Phone_number}}</small><br>
                                {% endif %}

                                {% if  TradeManager.Display_mobile_phone_number == true %}
                                <small><i class="fa fa-mobile"></i> {{TradeManager.Mobile_phone_number}}</small><br>
                                {% endif %}

                                {% if  TradeManager.Display_email == true %}
                                <small><i class="fa fa-envelope"></i>  {{TradeManager.Email}}</small><br>
                                {% endif %}

                                {% if  TradeManager.Display_communicator == true %}
                                <small><i class="fa fa-comment"></i> {{TradeManager.Communicator}}</small><br>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                    {% endif %}
                    <ul class="nav navbar-top-links navbar-left">
                        <li class="small-menu-list dropdown pull-left">
                            <a class="fusion-flyout-menu-toggle" data-toggle="dropdown" href="javascript:void(0);" aria-expanded="false">
                                <div style="width:100px">
                                    <div class="fusion-toggle-icon-line"></div>
                                    <div class="fusion-toggle-icon-line"></div>
                                    <div class="fusion-toggle-icon-line"></div>
                                </div>
                            </a>
                            <ul class="dropdown-menu">
                                {% for menuItem in MenuItems %}
                                {% if  menuItem.Use_reference == true %}
                                <li>
                                    {% if  menuItem.Use_link == true %}
                                    <a href="{{menuItem.Link}}">
                                        {% else %}
                                        <a href="{{menuItem.Link}}">
                                            {% endif %}
                                            {{menuItem.Name}}
                                        </a>
                                </li>
                                {% else %}
                                <li>
                                    <a>
                                        {{menuItem.Name}}
                                    </a>
                                    <ul>
                                        {% for subItems in menuItem.Menu_items %}
                                        <li class="second-menu-level">
                                            <div class="link-block">
                                                <a href="{{subItems.Link}}">
                                                    {{subItems.Name}}
                                                </a>
                                            </div>
                                        </li>
                                        {% endfor %}
                                    </ul>
                                </li>
                                {% endif %}
                                {% endfor %}
                            </ul>
                        </li>
                        {% for menuItem in MenuItems %}
                        {% if  menuItem.Use_reference == true %}
                        <li>
                            {% if  menuItem.Use_link == true %}
                            <a href="{{menuItem.Link}}">
                                {% else %}
                                <a href="{{menuItem.Link}}">
                                    {% endif %}
                                    {{menuItem.Name}}
                                </a>
                        </li>
                        {% else %}
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0);" aria-expanded="false">
                                <i class="fa fa-caret-down m-r-xxs" style="font-size:12px;"></i>      {{menuItem.Name}}
                            </a>
                            <ul class="dropdown-menu">
                                {% for subItems in menuItem.Menu_items %}
                                <li>
                                    <div class="link-block">
                                        <a href="{{subItems.Link}}">
                                            <strong>{{subItems.Name}}</strong>
                                        </a>
                                    </div>
                                </li>
                                {% endfor %}
                            </ul>
                        </li>
                        {% endif %}
                        {% endfor %}
                    </ul>
                    @BlockArea("Menu2")
                </div>
            </div>
        </div>
    </nav>
    <div id="wrapper" class="gray-bg" style="">
        <div class="row" style="">
            {% if  ShowLimits %}
            <div class="col-lg-12 ">
                <div class="row p-w-xl m-r-xs m-l-xs">
                    <div class="col-lg-12 limit-widget-carousel m-b-none">
                    </div>
                </div>
            </div>
            {% endif %}
            @BlockArea("BeforeContent")
            @RenderBody()
            @BlockArea("AfterContent")
        </div>
    </div>
</body>
<iframe id="downloadDocumentIframe" style="display:none;"></iframe>
<script>
    $(document).ready(function () {
        changeFavicon('{{ResAddress}}Res/Img?o={{OrganizationId}}&b={{FaviconId}}')
        $('.footable').footable();
        $(document).on('click', '.download-file', function (event) {
            var url = $(this).data('url');
            document.getElementById('downloadDocumentIframe').src = url;
        });
        $('.init-file-uploader').fileUploader({
        });
        $('#cart-navbar').cart({});
        setTimeout(function () {
            $(".truncate").dotdotdot({
                watch: 'window',
                wrap: 'letter'
            }, 0);
        });
        $('#article-search').search({
            favoredFilterSel: '#favored-filter-container'
        });
        setInterval(() => {
            $('.asynchronous-price-label').asynchronousPrice();
        }, 250);
        $('.comparison-tab').articleComparison();
    });
</script>

